{
  "framework": "vite",
  "installCommand": "cd frontend && npm install",
  "buildCommand": "cd frontend && npm run build",
  "outputDirectory": "frontend/dist",
  "env": {
    "VITE_WALLETCONNECT_PROJECT_ID": "@VITE_WALLETCONNECT_PROJECT_ID",
    "VITE_RPC_URL": "@VITE_RPC_URL",
    "VITE_CHAIN_ID": "@VITE_CHAIN_ID",
    "VITE_POOL_REGISTRY_ADDRESS": "@VITE_POOL_REGISTRY_ADDRESS",
    "VITE_LENDING_FACTORY_ADDRESS": "@VITE_LENDING_FACTORY_ADDRESS",
    "VITE_IDENTITY_VERIFIER_ADDRESS": "@VITE_IDENTITY_VERIFIER_ADDRESS",
    "VITE_MOCK_USDC_ADDRESS": "@VITE_MOCK_USDC_ADDRESS",
    "VITE_MOCK_USX_ADDRESS": "@VITE_MOCK_USX_ADDRESS"
  }
}
